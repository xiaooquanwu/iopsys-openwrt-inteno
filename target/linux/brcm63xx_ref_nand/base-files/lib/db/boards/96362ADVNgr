
config board 'board'
	option hardware 'DG150'
	option hasAdsl '1'
	option hasVdsl '0'
	option hasVoice '0'
	option hasDect '0'
	option VoicePorts '0'
	option VoiceLeds '0'
	option hasWifi '1'
	option ethernetPorts '5'
	option ethernetPortNames 'LAN1 LAN2 LAN3 LAN4 WAN'
	option ethernetPortOrder 'eth3 eth2 eth1 eth0 eth4'
	option ethernetLanPorts 'eth3 eth2 eth1 eth0'
	option ethernetWanPort 'eth4'
	option adslWanPort 'atm0'
	option lednames 'DSLWAN GbEWAN Internet Ethernet Status Wireless WPS ECO Voice'
	option buttonnames 'RESET WPS ECO'
	option wpsButtonGpio '11'

config wifi-chip '435f'
	option bands 'b'

config buttons 'buttons'
	option RESET 'gpio 9 al resetbutton'
	option WPS 'gpio 11 al wpsbutton'
	option ECO 'gpio 35 al ecobutton'

config leds 'leds'
	option DSLWAN_yellow 'sr 0 ah'
	option GbEWAN_yellow 'gpio 30 al'
	option GbEWAN_green 'gpio 29 al'
	option Internet_green 'gpio 1 al'
	option Internet_red 'sr 10 ah'
	option Ethernet_green 'sr 7 ah'
	option Status_green 'sr 2 ah'
	option Status_red   'sr 4 ah'
	option Wireless_green 'gpio 32 al'
	option WPS_green 'sr 5 ah'
	option WPS_red 'sr 6 ah'
	option ECO_green 'sr 8 ah'
	option Voice_green 'gpio 28 al'

config led_map 'led_map'
	option dsl_ok 'DSLWAN_yellow=ON'
	option dsl_off 'DSLWAN_yellow=OFF'
	option dsl_notice 'DSLWAN_yellow=BLINK_SLOW'
	option dsl_alert 'DSLWAN_yellow=BLINK_SLOW'
	option dsl_error 'DSLWAN_yellow=BLINK_FAST'
	option wifi_ok 'Wireless_green=ON'
	option wifi_off 'Wireless_green=OFF'
	option wifi_notice 'Wireless_green=BLINK_SLOW'
	option wifi_alert 'Wireless_green=BLINK_SLOW'
	option wifi_error 'Wireless_green=BLINK_FAST'
	option wps_ok 'WPS_green=ON'
	option wps_off 'WPS_green=OFF'
	option wps_notice 'WPS_green=BLINK_SLOW'
	option wps_alert 'WPS_green=OFF'
	option wps_error 'WPS_green=OFF'
	option lan_ok 'Ethernet_green=ON'
	option lan_off 'Ethernet_green=OFF'
	option lan_notice 'Ethernet_green=BLINK_SLOW'
	option lan_alert 'Ethernet_green=BLINK_SLOW'
	option lan_error 'Ethernet_green=BLINK_FAST'
	option status_ok 'Status_green=ON Status_red=OFF'
	option status_off 'Status_green=OFF Status_red=OFF'
	option status_notice 'Status_green=BLINK_SLOW Status_red=OFF'
	option status_alert 'Status_green=OFF Status_red=BLINK_SLOW'
	option status_error 'Status_green=OFF Status_red=BLINK_FASTF'
	option dect_ok ''
	option dect_off ''
	option dect_notice ''
	option dect_alert ''
	option dect_error ''
	option tv_ok ''
	option tv_off ''
	option tv_notice ''
	option tv_alert ''
	option tv_error ''
	option usb_ok ''
	option usb_off ''
	option usb_notice ''
	option usb_alert ''
	option usb_error ''
	option wan_ok ''
	option wan_off 'GbEWAN_green=OFF GbEWAN_yellow=OFF'
	option wan_notice 'GbEWAN_green=ON'
	option wan_alert 'GbEWAN_yellow=ON'
	option wan_error ''
	option internet_ok 'Internet_green=ON Internet_red=OFF'
	option internet_off 'Internet_green=OFF Internet_red=OFF'
	option internet_notice 'Internet_green=BLINK_SLOW Internet_red=OFF'
	option internet_alert 'Internet_green=OFF Internet_red=BLINK_SLOW'
	option internet_error 'Internet_green=OFF Internet_red=ON'
	option voice1_ok 'Voice_green=ON'
	option voice1_off 'Voice_green=OFF'
	option voice1_notice 'Voice_green=BLINK_SLOW'
	option voice1_alert 'Voice_green=BLINK_SLOW'
	option voice1_error 'Voice_green=BLINK_FAST'
	option voice2_ok ''
	option voice2_off ''
	option voice2_notice ''
	option voice2_alert ''
	option voice2_error ''
	option eco_ok 'ECO_green=ON'
	option eco_off 'ECO_green=OFF'
	option eco_notice 'ECO_green=BLINK_SLOW'
	option eco_alert 'ECO_green=BLINK_SLOW'
	option eco_error 'ECO_green=BLINK_FAST'

