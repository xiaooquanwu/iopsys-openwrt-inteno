
config board 'board'
	option hardware 'VG50'
	option hasAdsl '1'
	option hasVdsl '1'
	option hasVoice '0'
	option hasDect '0'
	option VoicePorts '0'
	option VoiceLeds '0'
	option hasWifi '1'
	option ethernetPorts '5'
	option ethernetPortNames 'LAN1 LAN2 LAN3 GbE WAN'
	option ethernetPortOrder 'eth2 eth1 eth0 eth4 eth3'
	option ethernetLanPorts 'eth2 eth1 eth0 eth4'
	option ethernetWanPort 'eth3'
	option adslWanPort 'atm0'
	option vdslWanPort 'ptm0'
	option lednames 'Status Ethernet WAN Wireless WPS USB DSL Internet GbE'
	option buttonnames 'RESET WPS WIFI'
	option wpsButtonGpio '33'

config buttons 'buttons'
	option RESET '32 al resetbutton'
	option WPS '33 al wpsbutton'
	option WIFI '34 al ecobutton'

config leds 'leds'
	option Status_green 'sr 4 ah'
	option Status_red 'sr 5 ah'
	option Ethernet_green 'sr 6 ah'
	option WAN_green 'gpio 19 al'
	option WAN_yellow 'gpio 20 al'
	option Wireless_green 'gpio 37 ah'
	option WPS_green 'sr 1 ah'
	option USB_green 'sr 7 ah'
	option DSL_red 'sr 0 ah'
	option Internet_green 'sr 10 ah'
	option Internet_red 'sr 2 ah'
	option GbE_green 'sr 8 ah'
	option GbE_yellow 'sr 9 ah'

config led_map 'led_map'
	option dsl_ok 'DSL_red=ON'
	option dsl_off 'DSL_red=OFF'
	option dsl_notice 'DSL_red=BLINK_SLOW'
	option dsl_alert 'DSL_red=BLINK_SLOW'
	option dsl_error 'DSL_red=BLINK_FAST'
	option wifi_ok 'Wireless_green=ON'
	option wifi_off 'Wireless_green=OFF'
	option wifi_notice 'Wireless_green=BLINK_SLOW'
	option wifi_alert 'Wireless_green=BLINK_SLOW'
	option wifi_error 'Wireless_green=BLINK_FAST'
	option wps_ok 'WPS_green=ON'
	option wps_off 'WPS_green=OFF'
	option wps_notice 'WPS_green=BLINK_SLOW'
	option wps_alert 'WPS_green=OFF'
	option wps_error 'WPS_green=OFF'
	option lan_ok 'Ethernet_green=ON'
	option lan_off 'Ethernet_green=OFF'
	option lan_notice 'Ethernet_green=BLINK_SLOW'
	option lan_alert 'Ethernet_green=BLINK_SLOW'
	option lan_error 'Ethernet_green=BLINK_FAST'
	option status_ok 'Status_green=ON Status_red=OFF'
	option status_off 'Status_green=OFF Status_red=OFF'
	option status_notice 'Status_green=BLINK_SLOW Status_red=OFF'
	option status_alert 'Status_green=OFF Status_red=BLINK_SLOW'
	option status_error 'Status_green=OFF Status_red=BLINK_FAST'
	option dect_ok ''
	option dect_off ''
	option dect_notice ''
	option dect_alert ''
	option dect_error ''
	option tv_ok ''
	option tv_off ''
	option tv_notice ''
	option tv_alert ''
	option tv_error ''
	option usb_ok 'USB_green=ON'
	option usb_off 'USB_green=OFF'
	option usb_notice 'USB_green=BLINK_SLOW'
	option usb_alert 'USB_green=BLINK_SLOW'
	option usb_error 'USB_green=BLINK_FAST'
	option wan_ok ''
	option wan_off 'WAN_green=OFF WAN_yellow=OFF'
	option wan_notice 'WAN_green=ON'
	option wan_alert 'WAN_yellow=ON'
	option wan_error ''
	option internet_ok 'Internet_green=ON Internet_red=OFF'
	option internet_off 'Internet_green=OFF Internet_red=OFF'
	option internet_notice 'Internet_green=BLINK_SLOW Internet_red=OFF'
	option internet_alert 'Internet_green=OFF Internet_red=BLINK_SLOW'
