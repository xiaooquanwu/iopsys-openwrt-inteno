
config wifi-device 'wl0'
        option type 'broadcom'
        option country 'EU/13'
        option band 'a'
        option bandwidth '40'
        option hwmode '11ac'
        option channel 'auto'
        option scantimer '120'
        option wmm '1'
        option wmm_noack '0'
        option wmm_apsd '0'
        option txpower '100'
        option rateset 'default'
        option frag '2346'
        option rts '2347'
        option dtim_period '1'
        option beacon_int '100'
        option rxchainps '0'
        option rxchainps_qt '10'
        option rxchainps_pps '10'
        option rifs '0'
        option rifs_advert '0'
        option maxassoc '32'
        option doth '1'
        option dfsc '1'

config wifi-iface
        option device 'wl0'
        option network 'lan'
        option mode 'ap'
        option ssid 'Inteno-$BSSID4'
        option encryption 'psk2'
        option cipher 'auto'
        option key '$WPAKEY'
        option gtk_rekey '3600'
        option macfilter '0'
        option wps_pbc '1'
        option wmf_bss_enable '1'
        option bss_max '16'

config wifi-iface
        option device 'wl0'
        option network 'lan'
        option mode 'ap'
        option ssid 'Inteno-$BSSID4-5G'
        option encryption 'psk2'
        option cipher 'auto'
        option key '$WPAKEY'
        option gtk_rekey '3600'
        option macfilter '0'
        option wps_pbc '0'
        option wmf_bss_enable '1'
        option bss_max '16'

config wifi-device 'wl1'
        option type 'broadcom'
        option country 'EU/13'
        option band 'b'
        option bandwidth '20'
        option hwmode 'auto'
        option channel 'auto'
        option scantimer '120'
        option wmm '1'
        option wmm_noack '0'
        option wmm_apsd '0'
        option txpower '100'
        option rateset 'default'
        option frag '2346'
        option rts '2347'
        option dtim_period '1'
        option beacon_int '100'
        option rxchainps '0'
        option rxchainps_qt '10'
        option rxchainps_pps '10'
        option rifs '0'
        option rifs_advert '0'
        option maxassoc '32'
        option doth '0'

config wifi-iface
        option device 'wl1'
        option network 'lan'
        option mode 'ap'
        option ssid 'Inteno-$BSSID4'
        option encryption 'psk2'
        option cipher 'auto'
        option key '$WPAKEY'
        option gtk_rekey '3600'
        option macfilter '0'
        option wps_pbc '1'
        option wmf_bss_enable '1'
        option bss_max '16'

config wifi-iface
        option device 'wl1'
        option network 'lan'
        option mode 'ap'
        option ssid 'Inteno-$BSSID4-2.4G'
        option encryption 'psk2'
        option cipher 'auto'
        option key '$WPAKEY'
        option gtk_rekey '3600'
        option macfilter '0'
        option wps_pbc '0'
        option wmf_bss_enable '1'
        option bss_max '16'
